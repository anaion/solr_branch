<?xml version="1.0" encoding="ISO-8859-1"?>
<config>
  <extension>.vt</extension>
  <encoding>UTF-8</encoding>
  <paths>
    <path>/templates/</path>
  </paths>
  <properties>
    <property name="velocimacro.library" value="templates/common/dateSelector.vt"/>
    <property name="velocimacro.library" value="templates/common/utils.vt"/>
    <property name="velocimacro.library" value="templates/common/options.vt"/>
    <property name="velocimacro.library" value="templates/common/result.vt"/>
    <property name="velocimacro.library" value="templates/common/table.vt"/>
    <property name="velocimacro.library" value="templates/common/Common.vt"/>
    <property name="file.resource.loader.cache" value="true"/>
    <property name="velocimacro.permissions.allow.inline" value="true"/>
    <property name="velocimacro.permissions.allow.inline.to.replace.global" value="false"/>
    <property name="velocimacro.permissions.allow.inline.local.scope" value="true"/>
    <property name="velocimacro.context.localscope" value="false"/>
    <property name="velocimacro.library.autoreload" value="false"/>
  </properties>
</config>